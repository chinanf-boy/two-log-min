{
	"name": "two-log-min",
	"version": "1.0.8",
	"description": "ora and debug , just two logger, if debug",
	"license": "MIT",
	"main": "dist/two-log-min.js",
	"module": "dist/two-log-min.m.js",
	"repository": "chinanf-boy/two-log-min",
	"author": {
		"name": "chinanf-boy",
		"email": "865501259@qq.com",
		"url": "http://llever.com"
	},
	"engines": {
		"node": ">=4"
	},
	"scripts": {
		"test": "npm run build && nyc ava -v -s",
		"build": "microbundle src/index.js && microbundle src/util.js -o dist/util.js && microbundle src/merge-opts.js -o dist/merge-opts.js",
		"precommit": "lint-staged",
		"npmUp": "npm version patch && npm publish",
		"git": "git push && git push --tags",
		"pub": "npm run test && npm run npmUp && npm run git"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"linters": {
			"*.{js,json,css,md}": [
				"prettier --single-quote --trailing-comma es5 --write",
				"git add"
			]
		},
		"ignore": [
			"**/dist/*.js"
		]
	},
	"size-limit": [
		{
			"path": "dist/two-log-min.js",
			"webpack": false,
			"gzip": false
		}
	],
	"files": [
		"dist/util.js",
		"dist/merge-opts.js"
	],
	"keywords": [
		"log",
		"ora",
		"debug"
	],
	"dependencies": {
		"debug": "^3.1.0",
		"get-module-name": "0.0.1",
		"ora-min": "^1.0.0",
		"yobrave-util": "^0.0.3"
	},
	"devDependencies": {
		"ava": "^0.20.0",
		"codecov": "^3.0.2",
		"husky": "1.0.0-rc.13",
		"lint-staged": "^7.0.0",
		"microbundle": "^0.6.0",
		"nyc": "^11.8.0"
	},
	"nyc": {
		"reporter": [
			"lcov",
			"text"
		]
	}
}
